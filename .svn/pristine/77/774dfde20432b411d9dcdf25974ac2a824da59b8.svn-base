package com.hx.common.base;


import java.io.Serializable;

/**
 * 请求公共对象，方便与后续扩展公共字段
 * @author daisi
 *
 */
public class CommonRequestBo implements Serializable {
	/**
	 * 
	 */
	private static final long serialVersionUID = -7858081831527950078L;

	private String data;
	
	private Page page;

	public String getData() {
		return data;
	}

	public void setData(String data) {
		this.data = data;
	}

	public Page getPage() {
		return page;
	}

	public void setPage(Page page) {
		this.page = page;
	}
	
}
